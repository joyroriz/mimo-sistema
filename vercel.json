{
  "version": 2,
  "builds": [
    {
      "src": "api/app_final.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app_final.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": ".",
    "CACHE_BUSTER": "FINAL-7.0.0-20250822-204000",
    "FORCE_REBUILD": "TRUE"
  },
  "functions": {
    "api/app_final.py": {
      "maxDuration": 30
    }
  },
  "github": {
    "silent": false
  },
  "cleanUrls": true,
  "trailingSlash": false
}
